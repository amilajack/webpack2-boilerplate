{
  "name": "webpack2-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for ES2015 and SASS using Webpack2. NOTE: 'webpack-validator' and 'webpack-bundle-size-analyzer' is not compatible with webpack-2.1.0-beta.25",
  "author": "Leif Olsen <leif.o.olsen.at.gmail.com> (https://github.com/leifoolsen)",
  "main": "webpack.config.babel.js",
  "scripts": {
    "setup": "npm install && npm-run-all clean validate",
    "clean": "rimraf dist",
    "validate": "npm-run-all lint test",
    "precommit": "npm run validate",
    "build:dll": "webpack --config webpack.config.dll.js",
    "prebuild": "npm-run-all clean validate build:dll",
    "build": "webpack --env.dev --progress",
    "prebuild:prod": "npm-run-all clean coverage",
    "build:prod": "NODE_ENV=production webpack --env.prod --progress",
    "lint": "npm-run-all lint:*",
    "lint:stylelint": "stylelint './src/**/*.s?(a|c)ss' --syntax scss",
    "lint:eslint": "eslint ./src ./server ./test",
    "coverage": "npm-run-all --parallel test:coverage --serial check:coverage",
    "test:coverage": "NODE_ENV=test babel-node ./node_modules/.bin/istanbul cover --root ./src _mocha --env.test -- --opts ./mocha.opts  './test/**/*.spec.js'",
    "check:coverage": "NODE_ENV=test istanbul check-coverage --statements 50 --branches 50 --functions 50 --lines 50",
    "test": "NODE_ENV=test mocha --env.test --opts ./mocha.opts './test/**/*.spec.js'",
    "test:watch": "npm run test -- --watch",
    "test:single": "NODE_ENV=test mocha  -watch --opts ./mocha.opts",
    "test:pattern": "npm run test -- --watch --grep $pattern",
    "analyze": "webpack --env.prod --json | webpack-bundle-size-analyzer",
    "server": "babel-node server --env.prod --port 8000",
    "start": "babel-node server --env.dev --hot --port 8084"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifoolsen/webpack2-boilerplate.git"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "moment": "2.17.1",
    "whatwg-fetch": "2.0.2"
  },
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-istanbul": "4.0.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "body-parser": "1.17.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.2",
    "eslint": "3.16.1",
    "eslint-loader": "1.6.3",
    "exports-loader": "0.6.4",
    "express": "4.15.0",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.3",
    "husky": "0.13.2",
    "ignore-styles": "5.0.1",
    "imports-loader": "0.7.1",
    "ip": "1.1.4",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.11.0",
    "json-loader": "0.5.4",
    "material-design-icons": "3.0.1",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "precss": "1.4.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "2.0.0",
    "rimraf": "2.6.1",
    "roboto-fontface": "0.7.0",
    "sass-loader": "6.0.2",
    "sinon": "1.17.7",
    "style-loader": "0.13.2",
    "stylelint": "7.9.0",
    "stylelint-config-standard": "16.0.0",
    "stylelint-webpack-plugin": "0.7.0",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-bundle-size-analyzer": "2.5.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.1"
  },
  "keywords": [
    "Webpack2",
    "Webpack",
    "JavaScript",
    "ES6",
    "ES2015",
    "ECMASCRIPT 2015",
    "Babel",
    "SASS",
    "SASS Boilerplate",
    "Mocha",
    "Chai",
    "Istanbul"
  ],
  "license": "MIT"
}
