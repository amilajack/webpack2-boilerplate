{
  "name": "webpack2-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for ES2015 and SASS using Webpack2. NOTE: 'webpack-validator' and 'webpack-bundle-size-analyzer' is not compatible with webpack-2.1.0-beta.25",
  "author": "Leif Olsen <leif.o.olsen.at.gmail.com> (https://github.com/leifoolsen)",
  "main": "webpack.config.babel.js",
  "scripts": {
    "setup": "npm install && npm-run-all clean validate",
    "clean": "rimraf dist",
    "validate": "npm-run-all lint test",
    "precommit": "npm run lint",
    "build:dll": "webpack --config webpack.config.dll.js",
    "prebuild": "npm-run-all clean validate build:dll",
    "build": "webpack --env.dev --progress",
    "prebuild:prod": "npm-run-all clean coverage",
    "build:prod": "NODE_ENV=production webpack --env.prod --progress",
    "lint": "npm-run-all lint:*",
    "lint:stylelint": "stylelint './src/**/*.s?(a|c)ss' --syntax scss",
    "lint:eslint": "eslint ./src ./server ./test",
    "coverage": "npm-run-all coverage:* check:coverage",
    "coverage:unit": "NODE_ENV=test babel-node ./node_modules/.bin/istanbul cover --root ./src --dir ./coverage/unit _mocha --env.test -- --opts ./mocha.opts './test/unit/**/*.spec.js'",
    "coverage:it": "NODE_ENV=test babel-node ./node_modules/.bin/istanbul cover --root ./src --dir ./coverage/integration _mocha --env.test -- --opts ./mocha.opts './test/integration/**/*.spec.js'",
    "coverage:report": "babel-node ./node_modules/.bin/istanbul report --root ./coverage --include **/coverage.raw.json lcov html",
    "check:coverage": "NODE_ENV=test istanbul check-coverage  --root coverage --statements 50 --branches 50 --functions 50 --lines 50",
    "test": "npm-run-all test:unit test:it",
    "test:unit": "NODE_ENV=test mocha --env.test --opts ./mocha.opts './test/unit/**/*.spec.js'",
    "test:it": "NODE_ENV=test mocha --env.test --opts ./mocha.opts './test/integration/**/*.spec.js'",
    "test:watch": "npm run test:unit -- --watch",
    "test:single": "NODE_ENV=test mocha --watch --opts ./mocha.opts",
    "test:pattern": "NODE_ENV=test mocha --watch --opts ./mocha.opts --grep $pattern",
    "analyze": "webpack --env.prod --json | webpack-bundle-size-analyzer",
    "server": "babel-node server --env.prod --port 8000",
    "start": "babel-node server --env.dev --hot --port 8084",
    "wdio": "babel-node ./node_modules/.bin/wdio test/wdio.conf.js"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifoolsen/webpack2-boilerplate.git"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "moment": "2.17.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.1",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-istanbul": "4.0.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-stage-0": "6.22.0",
    "body-parser": "1.17.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-http": "3.0.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.27.3",
    "cucumber": "2.0.0-rc.8",
    "eslint": "3.17.1",
    "eslint-loader": "1.6.3",
    "exports-loader": "0.6.4",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "husky": "0.13.2",
    "ignore-styles": "5.0.1",
    "imports-loader": "0.7.1",
    "ip": "1.1.5",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.12.0",
    "json-loader": "0.5.4",
    "material-design-icons": "3.0.1",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "precss": "1.4.0",
    "raw-loader": "0.5.1",
    "require-uncached": "1.0.3",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "roboto-fontface": "0.7.0",
    "sass-loader": "6.0.3",
    "selenium-standalone": "6.0.1",
    "sinon": "2.0.0",
    "style-loader": "0.14.1",
    "stylelint": "7.9.0",
    "stylelint-config-standard": "16.0.0",
    "stylelint-webpack-plugin": "0.7.0",
    "supertest": "3.0.0",
    "url-loader": "0.5.8",
    "wdio-cucumber-framework": "0.2.16",
    "wdio-mocha-framework": "0.5.9",
    "wdio-phantomjs-service": "0.2.2",
    "wdio-spec-reporter": "0.1.0",
    "webdriverio": "4.6.2",
    "webpack": "2.2.1",
    "webpack-bundle-size-analyzer": "2.5.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.1"
  },
  "keywords": [
    "Webpack2",
    "Webpack",
    "JavaScript",
    "ES6",
    "ES2015",
    "ECMASCRIPT 2015",
    "Babel",
    "SASS",
    "SASS Boilerplate",
    "Mocha",
    "Chai",
    "Istanbul"
  ],
  "license": "MIT"
}
